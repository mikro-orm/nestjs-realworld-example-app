{
  "name": "nestjs-realworld-example-app",
  "version": "1.0.0",
  "description": "NestJS + MikroORM realworld API example (MySQL)",
  "main": "index.js",
  "scripts": {
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --config=jest.json",
    "test:watch": "jest --watch --config=jest.json",
    "test:coverage": "jest --config=jest.json --coverage --coverageDirectory=coverage",
    "test:e2e": "APIURL=http://localhost:3000/api ./e2e/run-api-tests.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikro-orm/nestjs-realworld-example-app.git"
  },
  "author": "banan23@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mikro-orm/nestjs-realworld-example-app/issues"
  },
  "homepage": "https://github.com/mikro-orm/nestjs-realworld-example-app#readme",
  "dependencies": {
    "@mikro-orm/core": "^4.5.7",
    "@mikro-orm/mysql": "^4.5.7",
    "@mikro-orm/nestjs": "^4.2.0",
    "@mikro-orm/reflection": "^4.5.7",
    "@mikro-orm/sql-highlighter": "^1.0.1",
    "@nestjs/common": "^7.6.18",
    "@nestjs/core": "^7.6.18",
    "@nestjs/platform-express": "^7.6.18",
    "@nestjs/swagger": "^4.6.1",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "crypto": "^1.0.1",
    "crypto-js": "^4.0.0",
    "jsonwebtoken": "^8.5.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.3",
    "slug": "^3.3.5",
    "swagger-ui-express": "^4.1.4"
  },
  "devDependencies": {
    "@mikro-orm/cli": "^4.5.7",
    "@nestjs/cli": "^7.5.1",
    "@nestjs/schematics": "^7.1.2",
    "@nestjs/testing": "^7.6.18",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.2",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "atob": ">=2.1.2",
    "deep-extend": ">=0.6.0",
    "eslint": "^7.10.0",
    "eslint-plugin-import": "^2.22.1",
    "extend": ">=3.0.2",
    "jest": "^27.0.6",
    "supertest": "^5.0.0",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.0.0",
    "typescript": "4.3.5"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/mikro-orm.config.ts",
      "./dist/mikro-orm.config.js"
    ]
  }
}
